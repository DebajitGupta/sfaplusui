@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <nav class="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item active" aria-current="page">Customer Details Entry</li>
            </ol>
            <div class="ml-auto">
                <button type="button" class="btn bg-gradient-primary" ng-click="SubmitSubscriptionData(e)">Submit</button>
            </div>
        </nav>
    </div><!-- /.container-fluid -->
</div>


<div role="main">
    <div ng-controller="">


        <div class="col-lg-12">
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Add Customer</h3>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="form-group mb-0">
                                <select class="form-control" name="mname">
                                    <option>Select Customar Type</option>
                                    <option>option 2</option>
                                    <option>option 3</option>
                                    <option>option 4</option>
                                    <option>option 5</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <div class="form-group mb-0">
                                <select class="form-control" name="mname">
                                    <option>Select Parent Customer</option>
                                    <option>option 2</option>
                                    <option>option 3</option>
                                    <option>option 4</option>
                                    <option>option 5</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <div class="form-group">
                                <div class="custom-control custom-checkbox pt-2">
                                    <input class="custom-control-input" type="checkbox" id="customCheckbox1" value="option1">
                                    <label for="customCheckbox1" class="custom-control-label">Active</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-12">
            <div class="card card-primary">
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Customer Name">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Customar Contact No 2">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Customar Address 2">
                            </div>
                            <div class="form-group">
                                <select class="form-control" name="mname">
                                    <option>Select Zone</option>
                                    <option>option 2</option>
                                    <option>option 3</option>
                                    <option>option 4</option>
                                    <option>option 5</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text"><i class="fas fa-calendar-alt"></i></span>
                                    </div>
                                    <input type="date" class="form-control" name="edate" placeholder="Owner Aniversary">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Owner Name">
                            </div>
                            <div class="form-group">
                                <input type="email" class="form-control" name="noinfluencer" placeholder="Enter Customar Email-id">
                            </div>
                            <div class="form-group">
                                <select class="form-control" name="mname">
                                    <option>Select State</option>
                                    <option>option 2</option>
                                    <option>option 3</option>
                                    <option>option 4</option>
                                    <option>option 5</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter PIN Code">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Landmark">
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Customar Contact No 1">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" name="noinfluencer" placeholder="Enter Customar Address 1">
                            </div>
                            <div class="form-group">
                                <select class="form-control" name="mname">
                                    <option>Select District</option>
                                    <option>option 2</option>
                                    <option>option 3</option>
                                    <option>option 4</option>
                                    <option>option 5</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text"><i class="fas fa-calendar-alt"></i></span>
                                    </div>
                                    <input type="date" class="form-control" name="edate" placeholder="Owner Birthdate">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
